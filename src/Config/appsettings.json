{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.File"
    ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "C:\\Logs\\form_builder\\log-.log",
          "rollingInterval": "Day"
        }
      }
    ],
    "Enrich": [
      "FromLogContext"
    ]
  },
  "ElasticSearchAwsConfiguration": {
    "Region": "eu-west-1",
    "IndexFormat": "form_builder_service-local-{0:yyyy.MM-dd}",
    "InlineFields": false,
    "MinimumLevel": "Debug",
    "Enabled": false,
    "Url": "https://search-stockport-logs-4evarozpqvlofjsnxzliq7eqdy.eu-west-1.es.amazonaws.com/"
  },
  "Availability": {
    "BaseUrl": "http://scnavaildev.stockport.gov.uk/api/v1",
    "ErrorRoute": "/error/500",
    "WhitelistedRoutes": [
      "/swagger/index.html"
    ],
    "Environment": "local",
    "AllowSwagger": true
  },
  "FormConfig": {
    "DisallowedAnswerKeys": [
      "Path",
      "__RequestVerificationToken",
      "AddressStatus",
      "StreetStatus"
    ]
  },
  "DistrbutedCacheExpiration": 30,
  "HttpClientConfiguration": [
    {
      "iGatewayType": "StockportGovUK.AspNetCore.Gateways.IGateway",
      "gatewayType": "StockportGovUK.AspNetCore.Gateways.Gateway,StockportGovUK.AspNetCore.Gateways"
    },
    {
      "iGatewayType": "StockportGovUK.AspNetCore.Gateways.VerintServiceGateway.IVerintServiceGateway",
      "gatewayType": "StockportGovUK.AspNetCore.Gateways.VerintServiceGateway.VerintServiceGateway,StockportGovUK.AspNetCore.Gateways"
    }
  ]
}
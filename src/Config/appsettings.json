{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.File"
    ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "C:\\Logs\\form_builder\\log-.log",
          "rollingInterval": "Day"
        }
      }
    ],
    "Enrich": [
      "FromLogContext"
    ]
  },
  "ElasticSearchAwsConfiguration": {
    "Region": "eu-west-1",
    "IndexFormat": "form_builder-local-{0:yyyy.MM-dd}",
    "InlineFields": false,
    "MinimumLevel": "Debug",
    "Enabled": false,
    "Url": "https://search-stockport-logs-4evarozpqvlofjsnxzliq7eqdy.eu-west-1.es.amazonaws.com/"
  },
  "Availability": {
    "BaseUrl": "http://scnavaildev.stockport.gov.uk/api/v1",
    "ErrorRoute": "/error/500",
    "WhitelistedRoutes": [
      "/swagger/index.html"
    ],
    "Environment": "local",
    "AllowSwagger": true
  },
  "FormConfig": {
    "DisallowedAnswerKeys": [
      "Path",
      "__RequestVerificationToken",
      "AddressStatus",
      "StreetStatus",
      "OrganisationStatus"
    ]
  },
  "UseDistrbutedCache": false,
  "DistrbutedCacheExpiration": {
    "UserData": 30,
    "PaymentConfiguration": 5,
    "FormJson": 5,
    "FileUpload": 60
  },
  "HttpClientConfiguration": [
    {
      "iGatewayType": "StockportGovUK.NetStandard.Gateways.IGateway",
      "gatewayType": "StockportGovUK.NetStandard.Gateways.Gateway,StockportGovUK.NetStandard.Gateways"
    },
    {
      "iGatewayType": "StockportGovUK.NetStandard.Gateways.VerintServiceGateway.IVerintServiceGateway",
      "gatewayType": "StockportGovUK.NetStandard.Gateways.VerintServiceGateway.VerintServiceGateway,StockportGovUK.NetStandard.Gateways",
      "baseUrl": "http://scnwebdev1.stockport.gov.uk:8050/"
    },
    {
      "iGatewayType": "StockportGovUK.NetStandard.Gateways.Civica.Pay.ICivicaPayGateway",
      "gatewayType": "StockportGovUK.NetStandard.Gateways.Civica.Pay.CivicaPayTestGateway,StockportGovUK.NetStandard.Gateways",
      "baseUrl": "https://www.civicaepay.co.uk",
      "addPollyPolicies": "false"
    },
    {
      "iGatewayType": "StockportGovUK.NetStandard.Gateways.AddressService.IAddressServiceGateway",
      "gatewayType": "StockportGovUK.NetStandard.Gateways.AddressService.AddressServiceGateway,StockportGovUK.NetStandard.Gateways",
      "baseUrl": "https://dtstest.stockport.gov.uk/addressservice/"
    }
  ],
  "ErrorRoute": "/error"
}
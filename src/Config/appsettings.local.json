{
  "Serilog:Using": [
    "Serilog.Sinks.Console"
  ],
  "Serilog:WriteTo": [
    {
      "Name": "Console"
    }
  ],
  "Serilog:MinimumLevel:Override:Microsoft": "Information",
  "Serilog:MinimumLevel:Override:System": "Information",
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "SubmissionServiceConfiguration": {
    "FakeSubmission": true,
    "FakePaymentSubmission": true
  },
  "BookingApiConfiguration": {
    "BaseAddress": "https://localhost:44359/api/v1/",
    "GetAvailability": "Availability",
    "HasAvailability": "Availability/has-availability",
    "NextAvailability": "Availability/next-availability",
    "Cancellation": "Cancellation",
    "Confirmation": "Confirmation",
    "Reservation": "Reservation"
  },
  "IGatewayConfig:baseUrl": "",
  "IAddressServiceGatewayConfig:baseUrl": "http://scninthub-int1.stockport.gov.uk/addressservice/",
  "IStreetServiceGatewayConfig:baseUrl": "http://scninthub-int1.stockport.gov.uk/streetservice/",
  "IOrganisationGatewayServiceConfig:baseUrl": "http://scninthub-int1.stockport.gov.uk/organisationservice/",
  "ErrorRoute": "/error"
}
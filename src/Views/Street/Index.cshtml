@model form_builder.ViewModels.FormBuilderViewModel
@{
    ViewData["Title"] = Model.FormName;
    ViewData["PageTitle"] = Model.PageTitle;
    ViewData["FeedbackUrl"] = Model.FeedbackForm;
    ViewData["FeedbackPhase"] = Model.FeedbackPhase;
    ViewData["BaseUrl"] = $"../{Model.BaseURL}";
}

@using (Html.BeginForm("Index", "Street", FormMethod.Post))
{
    @Html.Raw(Model.RawHTML)

    @Html.HiddenFor(_ => _.Path)
    <input type="hidden" id="StreetStatus" name="StreetStatus" value="@Model.StreetStatus">
}
<script>
    function sendHeight() {
        const height = document.documentElement.scrollHeight || document.body.scrollHeight;
        let parentOrigin = "";
        if (window.location.ancestorOrigins && window.location.ancestorOrigins[0]) {
            parentOrigin = window.location.ancestorOrigins[0];
        } else {
            parentOrigin = document.referrer.split('/').slice(0, 3).join('/') + '/';
        }
        console.log("parent origin ", parentOrigin)

        const allowedOrigins = [
            "https://int-webplatform-stockroom.smbcdigital.net/",
            "https://qa-webplatform-stockroom.smbcdigital.net/",
            "https://stage-webplatform-stockroom.smbcdigital.net/",
            "https://www.stockrm.org/"
        ];

        if (allowedOrigins.includes(parentOrigin)) {
            console.log(`Sending height ${height} to ${parentOrigin}`);
            window.parent.postMessage({ iframeHeight: height }, parentOrigin);
        }
    }

    window.onload = sendHeight;
    window.onresize = sendHeight;
</script>


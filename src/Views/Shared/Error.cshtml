    @if (!Model.IsValid)
    {        
        var viewModel = new form_builder.ViewModels.ErrorViewModel
        {
            IsValid = Model.IsValid,
            Message =  string.IsNullOrEmpty(Model.Properties.CustomValidationMessage) ? Model.ValidationMessage : Model.Properties.CustomValidationMessage,
            Id = Model.ErrorId
        };

        <partial model=viewModel name="ErrorBase" />
    }
@{
    var displayHeading = (string)ViewData["Title"] != "Error";
}
<div class="center-wrapper" id="header-wrapper">
    <div id="header-logo-width">
        <div id="header-logo">
            <a href="https://www.stockport.gov.uk">Stockport Metropolitan Borough Council Homepage</a>
        </div>
    </div>
    @if (displayHeading)
    {
        <div id="formTitle">
            <a href="@Model.StartFormUrl"><strong>@Model.FormName</strong></a>
        </div>
    }
</div>
@{
    var feedbackFormUrl = (string)ViewData["FeedbackUrl"];
    var feedbackPhase = (string)ViewData["FeedbackPhase"];
}

<!doctype html>
<html lang="en" class="govuk-template flexbox no-flexboxtweener">
<head>
    <meta charset="utf-8" />
    <meta http-equiv="x-ua-compatible" content="ie=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <link rel="shortcut icon" type="image/png" href="~/assets/images/favicon-sg.png" media="print" />
    <title>@ViewData["PageTitle"] - @ViewData["Title"] - Stockport Council</title>
    <link href="https://ccfng4atgf.execute-api.eu-west-1.amazonaws.com/prod/int/1/smbc-frontend.min.css" rel="stylesheet" />
    <partial name="TagManagerHead" />
</head>

<body class="govuk-template__body  js-enabled">
    <partial name="TagManagerBody" />

    <script>document.body.className = ((document.body.className) ? document.body.className + ' js-enabled' : 'js-enabled');</script>
    
    <a href="#main-content" class="govuk-skip-link">Skip to main content</a>

    <partial name="ds-Header" />

    <div class="govuk-width-container">
        <main class="govuk-main-wrapper " id="main-content" role="main">
            <div class="govuk-grid-row">
                <div class="govuk-grid-column-two-thirds">
                    @RenderBody()  
                </div>
            </div>
        </main>
    </div>

    <partial name="ds-footer" />
    <script type="text/javascript" src="/assets/javascript/fileSizeCheck.js"></script>
    <script type="text/javascript" src="/assets/javascript/govuk-frontend-3.6.0.min.js"></script>
</body>

</html>